    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "query-string": "^6.0.0",
    "react": "^16.2.0",
    "react-ace": "^6.0.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "^16.6.1",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
